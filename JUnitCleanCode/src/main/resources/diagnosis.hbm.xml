<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "com.cedacri.pojo.Diagnosis" table = "Diagnosis">

        <meta attribute = "class-description">
            This class contains the diagnosis detail.
        </meta>
        <id name = "DoctorType" type = "string" column = "DoctorType">
            <generator class="native"/>
        </id>

        <property name = "DiagnosisName" column = "DiagnosisName" type = "string"/>
        <property name = "DoctorType" column = "DoctorType" type = "string" insert="false" update="false"/>

    </class>
</hibernate-mapping>